<launch>
    <arg name="sdf_robot_file" value="$(find jetPR_worlds)/models/person_standing/model.sdf"/>
    <arg name="robot_name" value="person_standing" />
    <arg name="x" default="1.0" />
    <arg name="y" default="1.0" />
    <arg name="z" default="0.0" />
    <arg name="yaw" default="0.0" />
    
    <include file="$(find person_sim)/launch/spawn_person.launch">
        <arg name="sdf_robot_file" value="$(arg sdf_robot_file)"/>
        <arg name="robot_name" value="$(arg robot_name)" />
        <arg name="x" value="$(arg x)" />
        <arg name="y" value="$(arg y)" />
        <arg name="z" value="$(arg z)" />
        <arg name="yaw" value="$(arg yaw)" />
    </include>
</launch>
